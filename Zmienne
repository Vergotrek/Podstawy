byte b = 12; // od -128 do 127
short s = 12345; // od -32768 do 32767
int i = 1212121212; // do 2147483647
long l = 12313141253453463; // 9 i 19 zer
float f = 3.444444f; // 38 po przecinku
double d = 3.7777777777777; // 108 po przecinku
char c = 'a'; // większość liter;
String st = "dowolny tekst"; // tekst 
boolean bo = false;  // zmienna przyjmujaca prawda/falsz - domyślnie false
